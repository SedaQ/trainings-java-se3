<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane xmlns="http://javafx.com/javafx/10.0.1"
	xmlns:fx="http://javafx.com/fxml"
	fx:controller="com.trainings.javafx.controllers.UserEditDialogController">
	<rowConstraints>
		<RowConstraints minHeight="20.0" prefHeight="30.0" />
		<RowConstraints minHeight="10.0" prefHeight="30.0" />
		<RowConstraints minHeight="10.0" prefHeight="30.0" />
		<RowConstraints minHeight="10.0" prefHeight="30.0" />
		<RowConstraints minHeight="10.0" prefHeight="30.0" />
		<RowConstraints minHeight="10.0" prefHeight="30.0" />
	</rowConstraints>
	<columnConstraints>
		<ColumnConstraints maxWidth="88.0" minWidth="10.0"
			prefWidth="83.0" />
		<ColumnConstraints maxWidth="167.0" minWidth="10.0"
			prefWidth="167.0" />
		<ColumnConstraints maxWidth="95.0" minWidth="0.0"
			prefWidth="0.0" />
		<ColumnConstraints maxWidth="95.0" minWidth="0.0"
			prefWidth="0.0" />
	</columnConstraints>
	<children>
		<Label text="email">
			<GridPane.margin>
				<Insets left="10.0" />
			</GridPane.margin>
		</Label>
		<Label text="nickname" GridPane.rowIndex="1">
			<GridPane.margin>
				<Insets left="10.0" />
			</GridPane.margin>
		</Label>
		<Label text="first name" GridPane.rowIndex="2">
			<GridPane.margin>
				<Insets left="10.0" />
			</GridPane.margin>
		</Label>
		<Label text="surname" GridPane.rowIndex="3">
			<GridPane.margin>
				<Insets left="10.0" />
			</GridPane.margin>
		</Label>
		<Label text="birthday" GridPane.rowIndex="4">
			<GridPane.margin>
				<Insets left="10.0" />
			</GridPane.margin>
		</Label>
		<TextField fx:id="emailTextField"
			maxWidth="1.7976931348623157E308" GridPane.columnIndex="1">
			<GridPane.margin>
				<Insets />
			</GridPane.margin>
		</TextField>
		<TextField fx:id="nickNameTextField"
			maxWidth="1.7976931348623157E308" GridPane.columnIndex="1"
			GridPane.rowIndex="1" />
		<TextField fx:id="firstNameTextField"
			maxWidth="1.7976931348623157E308" GridPane.columnIndex="1"
			GridPane.rowIndex="2" />
		<TextField fx:id="surnameTextField"
			maxWidth="1.7976931348623157E308" GridPane.columnIndex="1"
			GridPane.rowIndex="3" />
		<DatePicker fx:id="birthdayDatePicker"
			maxWidth="1.7976931348623157E308" GridPane.columnIndex="1"
			GridPane.rowIndex="4" />
		<HBox prefHeight="100.0" prefWidth="200.0"
			GridPane.columnIndex="1" GridPane.rowIndex="5">
			<children>
				<Button fx:id="okButton" minWidth="50.0"
					mnemonicParsing="false" text="OK" />
				<Button fx:id="cancelButton" minWidth="50.0"
					mnemonicParsing="false" text="Cancel" HBox.hgrow="ALWAYS">
					<HBox.margin>
						<Insets left="60.0" />
					</HBox.margin>
				</Button>
			</children>
		</HBox>
	</children>
	<padding>
		<Insets bottom="10.0" left="10.0" right="5.0" top="5.0" />
	</padding>
</GridPane>
